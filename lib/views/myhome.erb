<div id="main">
<h2>Agile fucking reflection, yo!</h2>

<a href='/new_entry' class="simple button">New Entry</a>

<p id="main">
<% user = User.first(username: env['warden'].user[:username]) %>
<% entries = Entry.all(user: user) %>
<% entries.reverse.each do |entry| %>
  <% entry_id = entry[:id] %>
  <div class="large-12 columns">
    <div class="callout">
    <a href="/view-entry/<%=entry_id%>"><%= entry[:created_at].year %>-<%= entry[:created_at].month %>-<%= entry[:created_at].day %></a>
    <br>
  </div>
  </div>
  <% end %>
</p>
</div>
</div>
